<UserControl
    x:Class="TMS.DeskTop.UserControls.Dialogs.Views.NewUserQuestionDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:hc="https://handyorg.github.io/handycontrol"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:work="clr-namespace:TMS.DeskTop.Views.Register.Question.WorkExperience"
    xmlns:local="clr-namespace:TMS.DeskTop.Views.Register"
    xmlns:data="clr-namespace:TMS.Core.Data;assembly=TMS.Core"
    xmlns:questions="clr-namespace:TMS.DeskTop.Views.Register.Question"
    xmlns:prism="http://prismlibrary.com/"
    Width="340"
    Height="500"
    d:DesignHeight="500"
    d:DesignWidth="340"
    MouseDown="UserControl_MouseDown"
    mc:Ignorable="d">
    <hc:SimplePanel x:Name="panel"
        FocusVisualStyle="{x:Null}"
        Focusable="True">
        <Border
            Background="White"
            CornerRadius="10"
            Effect="{StaticResource EffectShadow2}">
            <ContentControl prism:RegionManager.RegionName="{x:Static Member=data:RegionToken.QuestionContent}" />
        </Border>
    </hc:SimplePanel>
</UserControl>
