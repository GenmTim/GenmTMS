<base:RegionManagerControl
    x:Class="TMS.DeskTop.Views.WorkPlace.SubjectiveData.SubjectiveDataView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:base="clr-namespace:TMS.DeskTop.Tools.Base"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:prism="http://prismlibrary.com/"
    xmlns:token="clr-namespace:TMS.Core.Data.Token;assembly=TMS.Core"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:hc="https://handyorg.github.io/handycontrol"
    xmlns:local="clr-namespace:TMS.DeskTop.Views.WorkPlace.SubjectiveData"
    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <materialDesign:DialogHost
        CloseOnClickAway="True"
        Identifier="SubjectiveRoot"
        OverlayBackground="#3F000000">
        <ContentControl prism:RegionManager.RegionName="{x:Static Member=token:RegionToken.SubjectiveDataContent}" />
    </materialDesign:DialogHost>
</base:RegionManagerControl>
